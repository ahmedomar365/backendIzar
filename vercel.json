{
    "version": 2,
    "build" : [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": { "includeFiles": ["./build/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./build/index.js"
        }
    ]
}